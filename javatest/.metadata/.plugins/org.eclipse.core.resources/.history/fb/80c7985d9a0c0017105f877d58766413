package regex;

public class RegexDemo1 {
	private void mian() {
		// TODO Auto-generated method stub

		String str="1234567890";
		char[] ch=str.toCharArray();
		boolean flag=true;
		for(char c:ch)
		{
			if(c<'0'||c>'9')
			{
				flag=false;
				break;
			}
		
		}
		if(flag)
			System.out.println("++++++++++");
		else
			System.out.println("----------");
	}

}
